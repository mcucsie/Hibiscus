<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" defaultlocale="zh-Hant-TW" id="tw.edu.mcu.csie.Hibiscus" ios-CFBundleVersion="1.0.0"
        osx-CFBundleVersion="1.0.0" packageName="tw.edu.mcu.csie.Hibiscus" version="1.0.0"
        windows-packageVersion="1.0.0" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hibiscus</name>
    <description>Cloud Information System of MCU.</description>
    <author email="hibiscus@csie.mcu.edu.tw" href="https://github.com/mcucsie/Hibiscus">Hibiscus Team at MCU CSIE
    </author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <preference name="windows-target-version" value="10.0"/>
    <engine name="android" spec="~6.1.2"/>
    <engine name="ios" spec="~4.3.1"/>
    <engine name="windows" spec="~4.4.3"/>
    <engine name="osx" spec="~4.0.1"/>
    <engine name="ubuntu" spec="~4.3.4"/>
</widget>
